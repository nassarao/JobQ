@model JobQUI.Models.JobQViewModel
<table id="executedTable" class="table table-striped table-bordered table-inverse">
    <thead>
        <tr>
            @*<th>@Html.DisplayName("GUID")</th>*@
            <th>@Html.DisplayName("CMD")</th>
            <th>@Html.DisplayName("Arguments")</th>
            <th>@Html.DisplayName("Status")</th>
            <th>@Html.DisplayName("Date")</th>
            <th>@Html.DisplayName("Creating Client")</th>
            <th>@Html.DisplayName("Execution Client")</th>

        </tr>
    </thead>
    <tbody>
        @if (Model != null && Model.ExecutedJobs != null)
            {
                foreach (var item in Model.ExecutedJobs)
                {
                <tr data-toggle="modal" data-target="#JobDeleteModal" data-guid="@item.Id" data-job="@item.Cmd @item.Arguments">
                    @*<td>@item.GUID</td>*@
                    <td>@item.Cmd</td>
                    <td>@item.Arguments</td>
                    <td>@item.Status</td>
                    <td>@item.DateCreated</td>
                    <td>@item.CreatingMachine.Name</td>
                    @if (item.ExecutionMachine != null)
                    {
                        <td>@item.ExecutionMachine.Name</td>
                    }
                    else
                    {
                        <td></td>
                    }

                </tr>
            }
        }
    </tbody>
</table>